		event = { trigger_once = yes sound = { soundeffect = "distance_airplane_crash" } }

	}
	state = { name = "explode" animation = "idle" 
		event = { time = 0 particle = "vehicle_explode_effect" keep_particle = yes }
		event = { trigger_once = yes sound = { soundeffect = "distance_airplane_crash" } }

	}
	scale = 0.15
}

